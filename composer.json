{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "art35rennes/laravel-daisy-kit",
    "type": "library",
    "description": "Composants Blade (DaisyUI/Tailwind) pour Laravel.",
    "keywords": [
        "laravel",
        "blade",
        "components",
        "daisyui",
        "tailwindcss"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",

        "blade-ui-kit/blade-icons": "^1.8",
        "davidhsianturi/blade-bootstrap-icons": "^2.1",
        "illuminate/support": "^12.0",
        "illuminate/view": "^12.0"

    },
    "require-dev": {
        "laravel/framework": "^12.0",
        "phpunit/phpunit": "^11.5.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Art35rennes\\DaisyKit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Art35rennes\\DaisyKit\\DaisyKitServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
